<?xml version="1.0" encoding="UTF-8"?>
<datasources
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://www.ironjacamar.org/doc/schema'
    xsi:schemaLocation='http://www.ironjacamar.org/doc/schema http://www.ironjacamar.org/doc/schema/datasources_1_1.xsd'>
    <datasource jta="true" jndi-name="java:jboss/datasources/storeDS" enabled="true" use-java-context="true">
        <connection-url>jdbc:postgresql:storedb</connection-url>
        <driver>postgresql-driver</driver>
